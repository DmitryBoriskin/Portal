@model SettingsFrontModel
@{ 
    Layout = ViewBag.Layout;
}

<div class="content">
    <div class="block block_white block_padd block__shadow">
        <div class="block_head__blue">@Model.PageName</div>
        <div class="row">
            @{Html.RenderAction("GetChangeUserInfo", "Widget");}
            @{Html.RenderAction("GetChangePassword", "Widget");}
        </div>
        <div class="row">
            <div class="col-md-7">

                @try
                {
                    Html.RenderAction("Index", "LkWidgets", new { @area = "Lk" });
                }
                catch (Exception ex)
                {
                    //Log
                }
            </div>
        </div>
    </div>
    
   
</div>
